[global_config]
  homogeneous_tabbar = False
  scroll_tabbar = True
  window_state = fullscreen
[keybindings]
  copy = <Primary>c
  new_tab = <Primary>t
  paste = <Primary>v
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = Solarized Dark Higher Contrast
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#002b36"
    background_darkness = 1.0
    cursor_color = "#aaaaaa"
    font = Monospace 16
    foreground_color = "#839496"
    icon_bell = False
    palette = "#000000:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    show_titlebar = False
    use_system_font = False
  [[New Profile]]
    cursor_color = "#aaaaaa"
  [[Solarized Dark Higher Contrast]]
    background_color = "#001e27"
    copy_on_selection = True
    cursor_color = "#f34b00"
    font = Monaco 16
    foreground_color = "#9cc2c3"
    icon_bell = False
    palette = "#002831:#d11c24:#6cbe6c:#a57706:#2176c7:#c61c6f:#259286:#eae3cb:#006488:#f5163b:#51ef84:#b27e28:#178ec8:#e24d8e:#00b39e:#fcf4dc"
    scrollback_lines = 2000
    show_titlebar = False
    use_system_font = False
