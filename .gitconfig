[user]
	name = Sid Jayanna
	email = sjayanna@gmail.com
[alias]
	  co = checkout
	  ci = commit
	  st = status
	  br = branch -a
	  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
		history = log --oneline --decorate
	  type = cat-file -t
	  dump = cat-file -p
	  last = log -1 HEAD
	  unstage = reset HEAD
[core]
	autocrlf = false
	safecrlf = true
	excludesfile = /Users/username/.gitignore_global
	editor = vim
[color]
	ui = auto
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
